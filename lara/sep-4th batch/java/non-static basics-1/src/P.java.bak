class P 
{
	int i ;
	public static void main(String[] args) 
	{
		P p1 = new P();
		System.out.println(p1.i);
		P p2 = p1;
		p1.i = 20;
		System.out.println(p1.i);
		System.out.println(p2.i);
	}
}
//p1 and p2 are pointing to same object
//one object can have any number of reference variables
//here one object having 2 reference variables